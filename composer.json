{
    "name": "keshancs/admin-bundle",
    "type": "symfony-bundle",
    "description": "The Symfony CMS bundle",
    "keywords": [
        "symfony",
        "bundle",
        "admin",
        "adminLTE",
        "bootstrap",
        "CMS"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Georgijs Kļaviņš",
            "email": "georgijs.klavins@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1.3",
        "ext-intl": "*",
        "symfony-cmf/routing-bundle": "^2.4",
        "symfony/asset": "4.4.*",
        "symfony/form": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/security-bundle": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/translation": "4.4.*",
        "symfony/validator": "4.4.*"
    },
    "require-dev": {
        "symfony/var-dumper": "4.4.*"
    },
    "autoload": {
        "psr-4": {
            "AdminBundle\\": "src/"
        }
    }
}
